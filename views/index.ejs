<!-- clipboard -->
<script src="js/clipboard.min.js"></script>

<script>

  $(function () {
    $('[data-toggle="tooltip"]').tooltip()
  });

  var clipboard = new Clipboard('#copyToClipboard');

  clipboard.on('success', function(e) {

      if (e == undefined || e.text == undefined || e.text == "")
        return;

      toastr.info('Your GUIDs have been copied to your clipboard!');
      e.clearSelection();
  });

  clipboard.on('error', function(e) {
      toastr.error('Something went wrong and we could not access the clipboard.');
  });

</script>

